лее'                 on_release:                     root.manager.current ='secondscreen' <CountTeamsScreen>:      input2: input2     label:label     BoxLayout:         orientation:'vertical'         padding: 20         spacing: 50         canvas:             Color:                 rgba:0.95,0.94,0.9,1             Rectangle:                 pos: self.pos                 size: self.size         AnchorLayout:             anchor_x:'center'             anchor_y:'center'             size_hint: 1,0.1             Label:                 id:label                 color:0.36,0.54,0.66,1                 font_size:'18sp'                 size_hint: 0.5, 0.5                 text:'Введите количество команд'         AnchorLayout:             anchor_x:'center'             anchor_y:'center'             size_hint: 1, 0.1             TextInput:                 id:input2                 multiline: False                 input_type: 'number'                 input_filter: 'int'         AnchorLayout:             anchor_x:'right'             anchor_y:'bottom'             Button:                 size_hint: 0.2, 0.1                 text:'далее'                 on_release:                     root.get_teams_count() <TeamsNamesScreen>:     input_text: input     number_of_team: label     BoxLayout:         orientation:'vertical'         padding: 20         spacing: 50         canvas:             Color:                 rgba:0.95,0.94,0.9,1             Rectangle:                 pos: self.pos                 size: self.size         AnchorLayout:             anchor_x:'center'             anchor_y:'center'             size_hint: 1,0.1             Label:                 color:0.36,0.54,0.66,1                 font_size:'18sp'                 size_hint: 0.5, 0.5                 text:'Введите название команды'         AnchorLayout:             anchor_x:'center'             anchor_y:'center'             size_hint: 1,0.1             Label:                 id:label                 color:0.36,0.54,0.66,1     